<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ShopBundle\Entity\Customers" table="customers">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="200" nullable="false"/>
    <field name="surename" type="string" column="surename" length="300" nullable="false"/>
    <field name="patr" type="string" column="patr" length="300" nullable="false"/>
    <field name="adres" type="text" column="adres" nullable="false"/>
    <field name="contacts" type="string" column="contacts" length="300" nullable="false"/>
    <field name="phone" type="text" column="phone" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="created" type="date" column="created" nullable="false"/>
    <field name="modified" type="date" column="modified" nullable="false"/>
    <field name="createdBy" type="string" column="created_by" length="100" nullable="false"/>
    <field name="modifiedBy" type="string" column="modified_by" length="100" nullable="false"/>
    <field name="ownerId" type="integer" column="owner_id" nullable="false"/>
  </entity>
</doctrine-mapping>
